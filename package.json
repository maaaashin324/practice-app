  "scripts": {
    "start": "node index.js",
    "postinstall": "yarn build",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "makedb": "docker run -p 127.0.0.1:5432:5432 --name mydb -e POSTGRES_USER=ms -d postgres",
    "psql": "docker run -it --rm --link mydb:postgres postgres psql -h postgres -U ms",
    "startdb": "docker start mydb",
    "stopdb": "docker stop mydb"
  },
  "dependencies": {
    "express": "^4.16.3",
    "knex": "^0.14.6",
    "pg": "^7.4.3",
